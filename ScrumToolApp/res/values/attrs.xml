<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Sticker">
        <attr name="sticker_color" format="color" />
        <attr name="sticker_text" format="string" />
        <attr name="sticker_bold" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="Stamp">
        <attr name="stamp_color" format="color" />
        <attr name="stamp_content_quantity" format="string" />
        <attr name="stamp_content_unit" format="string" />
    </declare-styleable>
    
    <declare-styleable name="Slate">
        <attr name="slate_color" format="color" />
        <attr name="slate_title" format="string" />
        <attr name="slate_text" format="string" />
    </declare-styleable>
    
    <declare-styleable name="ExpandablePanel">
        <attr name="expandablepanel_handle" format="reference" />
        <attr name="expandablepanel_content" format="reference" />
        <attr name="expandablepanel_collapsedHeight" format="dimension" />
    </declare-styleable>

</resources>